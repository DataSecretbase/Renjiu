/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: rgb(241, 230, 230);
} 

.clearfix::after {
  content: '';
  clear: all;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.part {
  width: 100%;
  height: 160px;
  margin: 6px;
  background-color: #ffffff;
}